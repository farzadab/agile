<!-- Based on Ben's walker3d model -->
<!-- Source: https://github.com/belinghy/pybullet-custom-envs/blob/86c8cb8a0c94dff11478080d9b67ed3e083a92ca/cust_envs/walker3d.xml -->
<mujoco model="humanoid">
    <compiler angle="degree" inertiafromgeom="true"/>
    <default>
        <joint armature="0.01" damping=".1" limited="true"/>
        <geom conaffinity="1" condim="3" friction="1.2 0.1 0.1" contype="1" margin="0.001" material="geom" rgba=".9 .8 .4 1"/>
        <motor ctrllimited="true" ctrlrange="-.4 .4"/>
    </default>
    <option integrator="RK4" iterations="50" solver="PGS" timestep="0.003">
        <!-- <flags solverstat="enable" energy="enable"/>-->
    </option>
    <size nkey="5" nuser_geom="1"/>
    <visual>
        <map fogend="5" fogstart="3"/>
    </visual>
    <worldbody>
        <body name="torso" pos="0 0 1.35">

            <!-- F: make it 2D! -->
            <joint armature="0" axis="1 0 0" damping="0" limited="false" name="ignorex" pos="0 0 0" stiffness="0" type="slide"/>
            <joint armature="0" axis="0 0 1" damping="0" limited="false" name="ignorez" pos="0 0 0" ref="1.25" stiffness="0" type="slide"/>
            <joint armature="0" axis="0 1 0" damping="0" limited="false" name="ignorey" pos="0 0 0" stiffness="0" type="hinge"/>
            <!-- F -->

            <!--joint armature="0" damping="0" limited="false" name="root" pos="0 0 0" stiffness="0" type="free"/-->
            <body name="head" pos="0 0 .25">
                <geom name="head" pos="0 0 0" size=".1" type="sphere" user="258"/>
            </body>
            <geom name="torso1" pos="0 0 0" size=".14" type="sphere"/>
            <geom name="right_shoulder" pos="0 -0.13 0.08" size=".05" type="sphere"/>
            <geom name="left_shoulder" pos="0 0.13 0.08" size=".05" type="sphere"/>
            <body name="waist" pos="0 0 -0.240">
                <!--<geom fromto="0 -.06 0 0 .06 0" name="lwaist" size="0.06" type="capsule"/>-->

                <!-- F: get rid of extra DoFs for now -->
                <!-- <joint axis="0 0 1" name="abdomen_z" pos="0 0 0.065" range="-35 35" stiffness="0" type="hinge"/> -->
                <!-- <joint axis="0 1 0" name="abdomen_y" pos="0 0 0.065" range="-80 15" stiffness="0" type="hinge"/> -->
                <!-- F -->

                <geom name="waist" pos="0 0 0.07" size="0.09" type="sphere"/>
                <body name="pelvis" pos="0 0 -0.16" quat="1.000 0 -0.002 0">
                    <!-- F: get rid of extra DoFs for now -->
                    <!-- <joint axis="1 0 0" name="abdomen_x" pos="0 0 0.1" range="-25 25" stiffness="0" type="hinge"/> -->
                    <!-- F -->
                    <geom name="butt" pos="0 0 0.1" size="0.11" type="sphere"/>
                    <geom name="right_hip" pos="0 -0.1 0" size=".08" type="sphere"/>
                    <geom name="left_hip" pos="0 0.1 0" size=".08" type="sphere"/>
                    <!-- F-M: changed name from right_thigh -->
                    <body name="thigh" pos="0 -0.11 -0.04">

                        <!-- F: get rid of extra DoFs for now -->
                        <!-- <joint axis="1 0 0" name="right_hip_x" pos="0 0 0" range="-25 5" stiffness="0" type="hinge"/> -->
                        <!-- <joint axis="0 0 1" name="right_hip_z" pos="0 0 0" range="-40 35" stiffness="0" type="hinge"/> -->
                        <!-- <joint axis="0 1 0" name="right_hip_y" pos="0 0 0" range="-100 20" stiffness="0" type="hinge"/> -->
                        <!-- F: but let's keep right_hip_y under a different name ++ change the axis from 1 to -1 -->
                        <joint axis="0 -1 0" name="thigh_joint" pos="0 0 0" range="-100 20" stiffness="0" type="hinge"/>
                        <!-- F -->

                        <geom fromto="0 0 0 0 0.01 -.34" name="right_thigh1" size="0.065" type="capsule"/>
                        <!-- F-M: changed name from right_shin to leg -->
                        <body name="leg" pos="0 0.01 -0.403">
                            <!-- F-M: changed name from right_knee to leg_joint -->
                            <joint axis="0 -1 0" name="leg_joint" pos="0 0 .02" range="-150 -2" stiffness="0" type="hinge"/>
                            <geom fromto="0 0 0 0 0 -.34" name="right_shin1" size="0.055" type="capsule"/>
                            <!-- F-M: changed name from right_foot -->
                            <body name="foot" pos="0 0 -0.49">
                                <!-- F-M: changed name from right_ankle to foot_joint ++ change the axis from 1 to -1 -->
                                <joint axis="0 -1 0" name="foot_joint" pos="0 0 0.07" range="-20 40" stiffness="0" type="hinge"/>
                                <geom fromto="-0.02 0.02 0.07 0.17 0.03 0.07" name="right_foot_1" type="capsule" size="0.045"/>
                                <geom fromto="-0.02 -0.02 0.07 0.17 -0.03 0.07" name="right_foot_2" type="capsule" size="0.045"/>
                            </body>
                        </body>
                    </body>
                    <!-- F-M: changed name from left_thigh -->
                    <body name="thigh_left" pos="0 0.11 -0.04">
                        <!-- F: get rid of extra DoFs for now -->
                        <!-- <joint axis="-1 0 0" name="left_hip_x" pos="0 0 0" range="-25 5" stiffness="0" type="hinge"/> -->
                        <!-- <joint axis="0 0 -1" name="left_hip_z" pos="0 0 0" range="-40 35" stiffness="0" type="hinge"/> -->
                        <!-- <joint axis="0 1 0" name="left_hip_y" pos="0 0 0" range="-100 20" stiffness="0" type="hinge"/> -->
                        <!-- F: but let's keep left_hip_y under a different name ++ change the axis from 1 to -1 -->
                        <joint axis="0 -1 0" name="left_thigh_joint" pos="0 0 0" range="-100 20" stiffness="0" type="hinge"/>
                        <!-- F -->

                        <geom fromto="0 0 0 0 -0.01 -.34" name="left_thigh1" size="0.065" type="capsule"/>
                        <!-- F-M: changed name from left_shin -->
                        <body name="leg_left" pos="0 -0.01 -0.403">
                            <!-- F-M: changed name from left_knee -->
                            <joint axis="0 -1 0" name="leg_left_joint" pos="0 0 .02" range="-150 -2" stiffness="0" type="hinge"/>
                            <geom fromto="0 0 0 0 0 -.34" name="left_shin1" size="0.055" type="capsule"/>
                            <!-- F-M: changed name from left_foot -->
                            <body name="foot_left" pos="0 0 -0.49">
                                <!-- F-M: changed name from left_ankle ++ change the axis from 1 to -1 -->
                                <joint axis="0 -1 0" name="foot_left_joint" pos="0 0 0.07" range="-20 40" stiffness="0" type="hinge"/>
                                <geom fromto="-0.02 0.02 0.07 0.17 0.03 0.07" name="left_foot_1" type="capsule" size="0.045"/>
                                <geom fromto="-0.02 -0.02 0.07 0.17 -0.03 0.07" name="left_foot_2" type="capsule" size="0.045"/>
                            </body>
                        </body>
                    </body>
                </body>
            </body>

            <body name="right_upper_arm" pos="0 -0.17 0.08">
                <!-- F: get rid of extra DoFs for now -->
                <!-- <joint axis="1 0 0" name="right_shoulder1" pos="0 0 0" range="-90 120" stiffness="0" type="hinge"/> -->
                <!-- <joint axis="0 0 1" name="right_shoulder2" pos="0 0 0" range="-20 120" stiffness="0" type="hinge"/> -->
                <!-- F -->
                <geom fromto="0 0 0 0 -.27 0" name="right_uarm1" size="0.035" type="capsule"/>
                <body name="right_lower_arm" pos="0 -.27 0">
                    <!-- F: get rid of extra DoFs for now -->
                    <!-- <joint axis="0 0 1" name="right_elbow" pos="0 0 0" range="0 140" stiffness="0" type="hinge"/> -->
                    <!-- F -->
                    <geom fromto="0 0 0 0 -.27 0" name="right_larm" size="0.035" type="capsule"/>
                    <geom name="right_hand" pos="0 -.32 0" size=".04" type="sphere"/>
                </body>
            </body>
            <body name="left_upper_arm" pos="0 0.17 0.08">
                <!-- F: get rid of extra DoFs for now -->
                <!-- <joint axis="-1 0 0 " name="left_shoulder1" pos="0 0 0" range="-90 120" stiffness="0" type="hinge"/> -->
                <!-- <joint axis="0 0 -1" name="left_shoulder2" pos="0 0 0" range="-20 120" stiffness="0" type="hinge"/> -->
                <!-- F -->
                <geom fromto="0 0 0 0 .27 0" name="left_uarm1" size="0.035" type="capsule"/>
                <body name="left_lower_arm" pos="0 .27 0">
                    <!-- F: get rid of extra DoFs for now -->
                    <!-- <joint axis="0 0 -1" name="left_elbow" pos="0 0 0" range="0 140" stiffness="0" type="hinge"/> -->
                    <!-- F -->
                    <geom fromto="0 0 0 0 .27 0" name="left_larm" size="0.035" type="capsule"/>
                    <geom name="left_hand" pos="0 .32 0" size=".04" type="sphere"/>
                </body>
            </body>
       </body>
    </worldbody>
